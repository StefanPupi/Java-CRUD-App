<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PrikazFizickoLice</servlet-name>
        <servlet-class>FizickoLiceServlets.PrikazFizickoLice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFizickoLice</servlet-name>
        <servlet-class>FizickoLiceServlets.AddFizickoLice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrikazPravnoLice</servlet-name>
        <servlet-class>PravnoLiceServlets.PrikazPravnoLice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPravnoLice</servlet-name>
        <servlet-class>PravnoLiceServlets.AddPravnoLice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrikazServeri</servlet-name>
        <servlet-class>ServerServlets.PrikazServeri</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddFizickiServer</servlet-name>
        <servlet-class>ServerServlets.AddFizickiServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddVirtuelniServer</servlet-name>
        <servlet-class>ServerServlets.AddVirtuelniServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrikazPoslovanja</servlet-name>
        <servlet-class>Administracija.PrikazPoslovanja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditFizickoLice</servlet-name>
        <servlet-class>FizickoLiceServlets.EditFizickoLice</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrikazFizickoLice</servlet-name>
        <url-pattern>/PrikazFizickoLice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFizickoLice</servlet-name>
        <url-pattern>/AddFizickoLice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrikazPravnoLice</servlet-name>
        <url-pattern>/PrikazPravnoLice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPravnoLice</servlet-name>
        <url-pattern>/AddPravnoLice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrikazServeri</servlet-name>
        <url-pattern>/PrikazServeri</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddFizickiServer</servlet-name>
        <url-pattern>/AddFizickiServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddVirtuelniServer</servlet-name>
        <url-pattern>/AddVirtuelniServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrikazPoslovanja</servlet-name>
        <url-pattern>/PrikazPoslovanja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditFizickoLice</servlet-name>
        <url-pattern>/EditFizickoLice</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
